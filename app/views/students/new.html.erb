<h3>Student Form</h3>
<%= form_tag students_path do %>
<form action="<%=students_path%>" method="post">
  <label>First Name:</label> <br>
  <input id = "first_name" type="text" name="student[first_name]"><br>
  <label>Last Name:</label><br>
  <input id = "last_name" type="text" name="student[last_name]"><br>
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <input type="submit" value="Submit Student">
</form>
<% end %>
<%= params.inspect %>
