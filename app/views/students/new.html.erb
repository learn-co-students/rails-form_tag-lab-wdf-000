<h3>Student Form</h3>
  <%= form_tag students_path do %>
    <label>Student first_name:</label><br>
    <input type="text" id="first_name" name="student[first_name]"><br>

    <label>Student last_name:</label><br>
    <textarea id="last_name" name="student[last_name]"></textarea><br>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type="submit" value="Submit Student">
  <% end %>
</form>

<%= params.inspect %>
